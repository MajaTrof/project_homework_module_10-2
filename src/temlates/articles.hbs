
    <div class="row">
         {{#each articles}}
      <div class="card col-md-2">
        <div class="card-body">
          <h5 class="card-title">{{this.title}}</h5>
          <img class="card-img-top" src="{{this.urlToImage}}" alt="{{this.imageAlt}}">
          <p class="card-text">{{this.description}}</p>
          <p class="card-text">{{this.author}}</p>
        </div>
      </div>
       {{/each}}
    </div>
